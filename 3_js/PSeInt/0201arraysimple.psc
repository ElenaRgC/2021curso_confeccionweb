Algoritmo arraysimple
	
	Dimension array[5]
	
	Para i <- 1 Hasta 5 Hacer // Si no se indica, el paso es 1.
		Escribir "Introduzca un número."
		Leer array[i]
		
	FinPara
	
	Para i <- 1 Hasta 5 Hacer
		
		Escribir "El número que se encuentra en la posición " i " es: " array[i] "."
		
	FinPara
	
FinAlgoritmo